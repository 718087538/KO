/* KO/my/my5.wxss */
page{
    background: #fafafa;
}
.myOrder{
    margin: 30px 40rpx;
    background: #fff;
}
.myOrder .title{
    display: flex;
    padding:10rpx 30rpx;
    justify-content: space-between;
    height: 100rpx;
    align-items: center;
    border-bottom: 1px solid #eee;
}
.orderName{
    font-size: 36rpx;
}
.begin{
    font-size:24rpx;
    color: #fff;
    width: 130rpx;
    position: relative;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 20rpx;
    background-color: #4b90f6;margin-right:20rpx;
}
.myOrder .title .orderName::before{
    content: '';
    width: 5rpx;
    height: 32rpx;
    display: inline-block;
    background: #4bb5ff;
    margin-right: 30rpx;
}
.begin::after{
    content: '';
    position: absolute;
    width: 14rpx;
    height: 14rpx;
    top: 12rpx;
    right: -20rpx;
    background: transparent;
    border: 1rpx solid #b5b5b5;
    border-width: 2rpx 2rpx 0 0 ;
    transform: rotate(45deg);
}