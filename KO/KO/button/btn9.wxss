/* KO/button/btn9.wxss */

/* 隐藏的内容 */
.msgBg{
    position: fixed;
    top: 0;
    width: 100%;
    height: 1300rpx;
    background: rgba(0, 0, 0, 0.3)
}
.msgBg .content{
    width: 670rpx;
    height: 330rpx;
    margin: 200rpx auto 0;
    background: #fff;
    box-sizing: border-box;
    border-radius: 10rpx;
    overflow: hidden;
}
.msgBg .content .info{
    margin-top: 78rpx;
    text-align: center;
    font-size: 24rpx;
}
.msgBg .content .info .userName{
    font-size: 28rpx;
    color: red;
    margin: 0 10rpx;
}

.msgBg .content .go{
    width: 260rpx;
    background: #4b90f6;
    text-align: center;
    line-height: 70rpx;
    border-radius: 10rpx;
    margin: 80rpx auto 20rpx;
    color: #fff;
    font-size: 36rpx;
    height: 70rpx;
}