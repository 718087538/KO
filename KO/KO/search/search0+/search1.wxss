/* KO/search/search1.wxss */
.search{
    width: 100%;
    overflow: hidden;
    height: 240rpx;
    background-color: #09504c;
    padding: 30rpx 30rpx;
}
.img{
    width: 36rpx;
    height: 36rpx;
    margin: 0 0 0 30rpx;
}
.search-area{
    display: flex;
    align-items: center;
}
.input-cont{color: #fff;font-size: 28rpx;}
.input{
    height: 66rpx;
    width: 536rpx;
    background-repeat: no-repeat;
    border-radius: 30rpx;
    background-position:20rpx center;
    padding-left: 80rpx;
    background-image: url(data:image/png;base64,) ;
    background-color: #166c67;
}