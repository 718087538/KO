/* KO/search/search2.wxss */
.seach-box{
  width: 100%;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
}
.seach-box .logo{
  width: 113rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.seach-input{
  flex: 1;
  border: 2rpx solid #4c8beb;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #999;
  padding-left: 93rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 32rpx 32rpx;
  background-position: 30rpx center;
}