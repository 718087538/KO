/* KO/form/form17.wxss */

.myOrder{
    margin: 14px 0rpx;
    background: #fff;
}
.myOrder .title{
    display: flex;
    padding:10rpx 30rpx;
    justify-content: space-between;
    height: 100rpx;
    align-items: center;
    border-bottom: 1px solid #eee;
}
.orderName{
    font-size: 36rpx;
}
.begin{
    font-size:24rpx;
    width: 130rpx;
    position: relative;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 10rpx;
    border: 1rpx solid #4b90f6;
    color: #4b90f6;
}
.myOrder .title .orderName::before{
    content: '';
    width: 5rpx;
    height: 32rpx;
    display: inline-block;
    background: #4bb5ff;
    margin-right: 30rpx;
}
.myDate{
    margin: 20rpx 40rpx 70rpx;
    display: flex;
    justify-content: space-between;
}
.myDate .item image{
    width: 214rpx;
    height: 163rpx;
}
.myDate .item{
    width: 214rpx;
    position: relative;
    text-align: left;
}
.myDate .item view:nth-child(3){
    position: absolute;
    top: 64rpx;
    font-size: 60rpx;
    left: 20rpx;
}.myDate .item view:nth-child(1){
    font-size: 30rpx;
    margin-bottom: 20rpx;
}

.teamName{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 40rpx;
}
.teamName .name{
    font-size: 30rpx;
}
.teamName .teamInfo{
    flex:1;
    border-radius: 10rpx;
    height: 70rpx;
    padding: 0 14rpx;
    font-size: 28rpx;
    border: 2rpx solid #4b90f6;
}
.infoBox{
    padding: 40rpx;
}
.infoBox .infoArea{
     width:670rpx;
     margin-top: 30rpx;
    border-radius: 10rpx;
    height: 158rpx;
    padding:18rpx 30rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    border: 2rpx solid #4b90f6;
}
.Zpic{
    width: 218rpx;
    height: 228rpx;
}
.Zinfo{
    font-size: 24rpx;
    margin-bottom: 30rpx;
}
.zhengBox{
    padding: 40rpx;
}
.name .must{
    color: red;
    margin-right: 10rpx;
}