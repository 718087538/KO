/* KO/list/list3.wxss */
.box{
    display: flex;
    padding: 30rpx;
    border-bottom: 2rpx solid #f5f5f5;
}
.left{
    width: 100rpx;
    height: 100rpx;
    border-radius: 8rpx;
    margin-right: 20rpx;
}
.right{
display: flex;
flex-direction: column;
justify-content: space-between;
}
.title{
    font-size: 28rpx;
}
.title-name{
    display: flex;
    font-size: 24rpx;
}
.title-name view{
    margin-right: 20rpx;
    padding: 0rpx 10rpx;
    background-color: red;
    border-radius: 10rpx;
    color: #fff;
}