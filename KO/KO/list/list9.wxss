/* KO/list/list9.wxss */
page{
    background-color: #f5f5f5;
}
.box{
    margin: 20rpx 0;
    padding: 20rpx;
    display: flex;
    flex-wrap: wrap;
    font-size: 32rpx;
    justify-content: space-between;
    background-color: #fff;
}

.right{
    display: flex;
    align-items: center;
}
.right-title{
    color: #808080;
}
.right-img{
    display: block;
    width: 32rpx;
    height: 32rpx;
    margin-left: 20rpx;
}

.bottom-img{
    width: 166rpx;
    height: 66rpx;
    margin-left: 50%;
    margin-top: 30rpx;
    transform: translateX(-50%);
}

.text-info{
    color: #808080;
    margin: 0 30rpx;
    font-size: 24rpx;
}
.active{
    color: #c99034;
}