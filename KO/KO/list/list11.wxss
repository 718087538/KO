/* KO/button/button4.wxss */
page{
    background-color:#f5f5f5;
}
.title-img{
    width: 94rpx;
    height: 94rpx;
    margin-left: 40rpx;
    margin-right: 8rpx;
    border-radius: 100% !important;
}
.jiao{
    width: 30rpx;
    height: 48rpx;
    margin-top: 30rpx;
    position: relative;
    z-index: 2;
}
.box{
    display: flex;
    margin-top: 30rpx;
    padding-right: 50rpx;
}
.question{
    flex: 1;
    background-color: #fff;
    border-radius: 20rpx;
    padding: 26rpx;
    box-shadow: 0 0 8rpx 8rpx rgb(240, 240, 240);
    font-size: 28rpx;
}
.time{
    display: block;
    margin-top: 20rpx;
    color: #09504c;
}