/* KO/list/list14.wxss */
.myOrder{
    margin: 14px 0rpx;
    background: #fff;
}
.myOrder .title{
    display: flex;
    padding:10rpx 30rpx;
    justify-content: space-between;
    height: 100rpx;
    align-items: center;
    border-bottom: 1px solid #eee;
}
.orderName{
    font-size: 36rpx;
}
.begin{
    font-size:24rpx;
    width: 130rpx;
    position: relative;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 10rpx;
    border: 1rpx solid #4b90f6;
    color: #4b90f6;
}
.myOrder .title .orderName::before{
    content: '';
    width: 5rpx;
    height: 32rpx;
    display: inline-block;
    background: #4bb5ff;
    margin-right: 30rpx;
}
