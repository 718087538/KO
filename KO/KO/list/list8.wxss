/* KO/list/list8.wxss */
.box{
    display: flex;
    align-items: center;
    padding: 30rpx;
    border-bottom: 2rpx solid #f5f5f5;
    font-size: 28rpx;
}
.box-img{
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 30rpx;
}
.title{
    width: 200rpx;
}
.state{
    flex: 1;
    text-align: right;
    color: #09504c;
    font-weight: bold;
}