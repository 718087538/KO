/* KO/title/title3.wxss */
page{
    background-color: #f5f5f5;
}
.box{
    background-color: #fff;
    overflow: hidden;
    border-radius: 10rpx;
    margin: 30rpx 20rpx 20rpx 20rpx;
}
.box-img{
    display: block;
    width: 196rpx;
    height: 120rpx;
    margin: 30rpx auto 20rpx;
}
.box-title1,.box-title2{
    font-size: 24rpx;
    text-align: center;
    color: #b3b3b3;
}
.box-title2{
    color: #fff;
    width: 200rpx;
    margin: 30rpx auto;
    padding: 10rpx 10rpx;
    border-radius: 30rpx;
    background-color: #ff759c;
}