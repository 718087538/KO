    display: flex;
    align-content: center;
    text-align: center;
    padding-top: 40rpx;    
}
.faBuBox .manage{
    margin-right:90rpx;
    padding-right: 80rpx;
    margin-left: 100rpx;
    height: 170rpx;
    border-right: 2rpx solid #f2f2f2;
}
.faBuBox .manageImg{
    width: 36rpx;
    height: 43rpx;
}
.faBuBox .name{
    font-size: 20rpx;
