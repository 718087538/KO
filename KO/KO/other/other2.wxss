/* KO/other/other2.wxss */
.honorBox{
    padding: 50rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 2rpx solid #f2f2f2
}
.honorItem{
    text-align: center;
    width: 222rpx;
}
.honorImg{
    display: block;
    width: 155rpx;
    height: 200rpx;
    margin: 0 auto 46rpx;
}
.honorBox .state{
    font-size: 36rpx;color: #4b90f6;
    margin-bottom: 20rpx;
}
.honorBox .time{
    font-size: 24rpx;
}
.honorBox .nuli{
    font-size: 24rpx;
    color: #666;
}