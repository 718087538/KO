/* KO/address/address2.wxss */
page{
    background-color: #f5f5f5;
    color: #333;
}
.box{
    margin: 20rpx;
    display: flex;
    padding: 20rpx;
    background-color: #fff;
    border-bottom: 2rpx solid #fafafa;
}
.box-left-img{
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
}
.box-right{
    flex: 1;
}
.box-right-title{
    font-size: 32rpx;
    font-weight: bold;
}
.box-right-info{
    font-size: 20rpx;
    margin-top: 10rpx;
}